<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>demoServlet</servlet-name>
  	<display-name>demoServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.DemoServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletComunicador</servlet-name>
  	<display-name>ServletComunicador</display-name>
  	<description></description>
  	<servlet-class>servlet.ServletComunicador</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletController</servlet-name>
  	<display-name>ServletController</display-name>
  	<description></description>
  	<servlet-class>servlet.ServletController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>demoServlet</servlet-name>
  	<url-pattern>/demoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletComunicador</servlet-name>
  	<url-pattern>/ServletComunicador</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletController</servlet-name>
  	<url-pattern>/ServletController</url-pattern>
  </servlet-mapping>
</web-app>
